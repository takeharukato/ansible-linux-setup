#
# Multus設定値
#
image:
  # リポジトリ名
  repository: bitnami/multus-cni
  # multus CNIコンテナイメージの版数
  tag: {{k8s_multus_image_version}}
  useDigest: false
  suffix: ""
  imageSuffix: ""

cni:
  # CNIのバイナリファイル配置ディレクトリ
  binDir: /opt/cni/bin
  # CNIのコンフィグレーションファイル配置先ディレクトリ
  # ワーカノード上で`ls /etc/cni/net.d`を実行し,
  # 00-multus.conf, multus.dができていることを確認する
  # 実行例
  # ansible@k8sworker01 $ ls /etc/cni/net.d
  # 00-multus.conf  05-cilium.conflist  multus.d  whereabouts.d
  confDir: /etc/cni/net.d